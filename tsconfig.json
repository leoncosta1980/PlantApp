{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,

    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@/*": ["./*"]
    },
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "types": [
      "node"
    ],
    "target": "ES6",
    "lib": [
      "ES6",
      "DOM",
      "ES2015",
      "ScriptHost"
    ]    
  }
}
